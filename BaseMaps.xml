        <Metadata>
          <Meta name="Description">USGS HUC Watershed Boundaries</Meta>
        </Metadata>
      </Layer>
      <Layer name="Drainage Network NHD" url="https://hydro.nationalmap.gov:443/arcgis/services/nhd/MapServer/WmsServer?">
        <Metadata>
          <Meta name="Description">USGS National Hydrologic Dataset - Drainage Network</Meta>
        </Metadata>
      </Layer>
    </GroupLayer>
    <GroupLayer name="USGS Topography">
      <Layer name="Topo Base Map" url="https://basemap.nationalmap.gov:443/arcgis/services/USGSTopo/MapServer/WmsServer?">
        <Metadata>
          <Meta name="Description">USGS Top Map Base Layer</Meta>
        </Metadata>
      </Layer>
      <Layer name="Shaded Relief" url="https://basemap.nationalmap.gov:443/arcgis/services/USGSShadedReliefOnly/MapServer/WmsServer?">
