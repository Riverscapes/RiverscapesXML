<?xml version="1.0" encoding="utf-8"?>
<Project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://xml.riverscapes.xyz/RaveBusinessLogic/XSD/project_explorer.xsd">
    <!-- <Project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="./XSD/project_explorer.xsd"> -->
    <Name>TauDEM</Name>
    <ProjectType>TauDEM</ProjectType>
    <Node xpathlabel="Name" xpath="Realizations/TauDEM">
        <Children collapsed="false">
             <Node label="Inputs" xpath="Inputs">
                <Children collapsed="true">
                  <Node label="DEM" xpath="Raster[@id='DEM']" type="raster" symbology="dem" transparency="40" id="dem" />
                  <Node label="Channel Area" xpath="Geopackage/Layers/Vector[@id='CHANNEL_AREA']" type="polygon" id="channel_area" symbology="ChannelArea" />
                </Children>
            </Node>
            <Node label="Intermediates" xpath="Intermediates">
                <Children collapsed="true">
                    <Node label="Rasterized Channel" xpath="Raster[@id='RASTERIZED_CHANNEL']" type="raster" symbology="rasterizedchannel" transparency="40" />
                </Children>
            </Node>
            <Node label="Outputs">
                <Children collapsed="false">
                    <Node label="TauDEM Outputs">
                        <Children collapsed="false">
                            <Node label="Slope Analysis (D∞)" xpath="Outputs/Raster[@id='DINFFLOWDIR_SLP']" type="raster" symbology="dinfflowdir_slp" transparency="40" />
                            <Node label="DEM Hillshade" xpath="Inputs/Raster[@id='HILLSHADE']" type="raster" symbology="hillshade" id="hillshade" />
                            <Node label="Basic Grid Analysis">
                                <Children collapsed="true">
                                    <Node label="Pitfill DEM" xpath="Intermediates/Raster[@id='PITFILL']" type="raster" symbology="dem" transparency="40" />
                                    <Node label="Flow Directions (D∞)" xpath="Intermediates/Raster[@id='DINFFLOWDIR_ANG']" type="raster" symbology="dinfflowdir_ang" transparency="40" />
                                    <Node label="Contributing Area (D∞)" xpath="Intermediates/Raster[@id='AREADINF_SCA']" type="raster" symbology="areadinf_sca" transparency="40" />
                                </Children>
                            </Node>
                            <Node label="Specialized Grid Analysis">
                                <Children collapsed="false">
                                    <Node label="TWI" xpath="Outputs/Raster[@id='TWI_RASTER']" type="raster" symbology="twi" transparency="40" />
                                    <Node label="HAND" xpath="Outputs/Raster[@id='HAND_RASTER']" type="raster" symbology="hand" transparency="40" />
                                </Children>
                            </Node>
                        </Children>
                    </Node>
                    <!-- commenting out GDAL unit nodes ready
                    <Node label="GDAL Outputs">
                        <Children>
                             Node label="Slope Analysis (D8)" xpath="" type="raster" symbology="" transparency="" id="d8_slope" />
                        </Children>
                    </Node>
                -->
                </Children>
            </Node>
        </Children>
    </Node>
    <Views default="DEFAULT">
        <View name="TauDEM Default View" id="DEFAULT">
            <Layers>
                <Layer id="DINFFLOWDIR_SLP" />
            </Layers>
        </View>
    </Views>
</Project>