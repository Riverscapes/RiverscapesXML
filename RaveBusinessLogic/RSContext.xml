<?xml version="1.0" encoding="utf-8"?>
<!-- <Project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://xml.riverscapes.xyz/RaveBusinessLogic/XSD/project_explorer.xsd"> -->
<Project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="./XSD/project_explorer.xsd">
  <Name>Riverscapes Context</Name>
  <ProjectType>RSContext</ProjectType>
  <Node xpathlabel="Name" xpath="Realizations/RSContext">
    <Children collapsed="false">
      <Node xpathlabel="Name" xpath="Realizations/RSContext/HTMLFile[@id='REPORT']" type="file" />
      <Node label="Transportation Local">
        <Children collapsed="true">
          <Node xpathlabel="Name" xpath="Realizations/RSContext/Vector[@id='Roads']" type="line" symbology="roads" />
          <Node xpathlabel="Name" xpath="Realizations/RSContext/Vector[@id='Rail']" type="line" symbology="railroads" />
        </Children>
      </Node>
      <Node label="Hydrology">
        <Children collapsed="false">
          <Node label="Hydrography (NHD HR+)">
            <Children collapsed="true">
              <Node label="Perrenial Drainage Network" xpath="Realizations/RSContext/Vector[@id='NHDFlowline']" type="line" symbology="nhdperrenial" />
              <Node label="Intermittent Drainage Network" xpath="Realizations/RSContext/Vector[@id='NHDFlowline']" type="line" symbology="nhdintermittent" />
              <Node label="Ephemeral Drainage Network" xpath="Realizations/RSContext/Vector[@id='NHDFlowline']" type="line" symbology="nhdephemeral" />
              <Node label="Network - Upstream Drainage Area" xpath="Realizations/RSContext/Vector[@id='NHDFlowline']" type="line" symbology="upstreamdrainagearea" />
              <Node label="Full NHD HR+ Drainage Network" xpath="Realizations/RSContext/Vector[@id='NHDFlowline']" type="line" symbology="flow_lines" />
              <Node label="Drainage Wings" xpath="Realizations/RSContext/Vector[@id='NHDPlusCatchment']" type="polygon" symbology="drainagewings" />
              <Node label="River Polygons (NHD Area)" xpath="Realizations/RSContext/Vector[@id='NHDArea']" type="polygon" symbology="nhdarea" />
              <Node label="Lakes, Ponds, Reservoirs and Water Bodies" xpath="Realizations/RSContext/Vector[@id='NHDWaterbody']" type="polygon" symbology="waterbody" />
              <Node label="Canals and Ditches" xpath="Realizations/RSContext/Vector[@id='Canals']" type="line" symbology="canals_ditches" />
              <Node label="Pipes and Ditches" xpath="Realizations/RSContext/Vector[@id='NHDFlowline']" type="line" symbology="pipes" />
              <Node label="Estimated Bankfull">
                <Children>
                  <Node xpathlabel="Name" xpath="Realizations/RSContext/Geopackage/Layers/Vector[@id='NETWORK']" type="line" symbology="bankfull_flowlines"/>
                  <Node xpathlabel="Name" xpath="Realizations/RSContext/Geopackage/Layers/Vector[@id='BANKFULL_CHANNEL']" type="polygon" symbology="bankfull_channel"/>
                  <!-- <Node xpathlabel="Name" xpath="Realizations/RSContext/Geopackage/Layers/Vector[@id='COMPOSITE_CHANNEL_AREA']" type="polygon" symbology="bankfull_nhd"/> -->
                </Children>
              </Node>
              <Node label="Segmented Hydrography">
                <Children>
                  <Node xpathlabel="Name" xpath="Realizations/RSContext/Geopackage/Layers/Vector[@id='NETWORK300M']" type="line" symbology="" />
                  <Node xpathlabel="Name" xpath="Realizations/RSContext/Geopackage[@id='NHD']/Layers/Vector[@id='NETWORK300MCROSSINGS']" type="line" symbology="" />
                </Children>
              </Node>
            </Children>
          </Node>
          <Node label="Watershed Boundaries (HUCs)">
            <Children collapsed="true">
              <Node label="Region - 2-Digit HUC" xpath="Realizations/RSContext/Vector[@id='WBDHU2']" type="polygon" symbology="wbdhu2" />
              <Node label="Subregion - 4-Digit HUC" xpath="Realizations/RSContext/Vector[@id='WBDHU4']" type="polygon" symbology="wbdhu4" />
              <Node label="Basin - 6-Digit HUC" xpath="Realizations/RSContext/Vector[@id='WBDHU6']" type="polygon" symbology="wbdhu6" />
              <Node label="Sub-Basin - 8-Digit HUC" xpath="Realizations/RSContext/Vector[@id='WBDHU8']" type="polygon" symbology="wbdhu8" />
              <Node label="Watersheds - 10-Digit HUC" xpath="Realizations/RSContext/Vector[@id='WBDHU10']" type="polygon" symbology="wbdhu10" />
              <Node label="Sub-Watersheds - 12-Digit HUC" xpath="Realizations/RSContext/Vector[@id='WBDHU12']" type="polygon" symbology="wbdhu12" />
            </Children>
          </Node>
        </Children>
      </Node>
      <Node label="Climate (PRISM)">
        <Children collapsed="true">
          <Node label="Mean Annual Precipitation (mm)" xpath="Realizations/RSContext/Raster[@id='Precip']" type="raster" symbology="mean_annual_precip" transparency="50" />
          <Node label="Mean Temperature (째C)" xpath="Realizations/RSContext/Raster[@id='MeanTemp']" type="raster" symbology="temperature" transparency="50" />
          <Node label="Minimum Temperature (째C)" xpath="Realizations/RSContext/Raster[@id='MinTemp']" type="raster" symbology="temperature" transparency="50" />
          <Node label="Maximum Temperature (째C)" xpath="Realizations/RSContext/Raster[@id='MaxTemp']" type="raster" symbology="temperature" transparency="50" />
          <Node label="Mean Dewpoint Temperature (째C)" xpath="Realizations/RSContext/Raster[@id='MeanDew']" type="raster" symbology="temperature" transparency="50" />
          <Node label="Minimum Vapor Pressure Deficit (hPa)" xpath="Realizations/RSContext/Raster[@id='MinVap']" type="raster" symbology="vapor_pressure" transparency="50" />
          <Node label="Maximum Vapor Pressure Deficit (hPa)" xpath="Realizations/RSContext/Raster[@id='MaxVap']" type="raster" symbology="vapor_pressure" transparency="50" />
        </Children>
      </Node>
      <Node label="Ecoregions">
        <Children collapsed="true">
          <Node label="Level 1 Ecoregions" xpath="Realizations/RSContext/Vector[@id='Ecoregions']" type="polygon" symbology="ecoregions_1" transparency="40" />
          <Node label="Level 2 Ecoregions" xpath="Realizations/RSContext/Vector[@id='Ecoregions']" type="polygon" symbology="ecoregions_2" transparency="40" />
          <Node label="Level 3 Ecoregions" xpath="Realizations/RSContext/Vector[@id='Ecoregions']" type="polygon" symbology="ecoregions_3" transparency="40" />
        </Children>
      </Node>
      <Node label="Land Management">
        <Children collapsed="true">
          <Node label="Land Ownership" xpath="Realizations/RSContext/Vector[@id='Ownership']" type="polygon" symbology="ownership" transparency="40" />
          <Node label="Fair Market Value" xpath="Realizations/RSContext/Raster[@id='FAIRMARKETVALUE']" type="raster" symbology="fair_market" transparency="40" />
        </Children>
      </Node>
      <Node label="Vegetation">
        <Children>
          <Node label="Existing Vegetation">
            <Children collapsed="true">
              <Node label="Veg Type - EVT Type" xpath="Realizations/RSContext/Raster[@id='EXVEG']" type="raster" symbology="Existing_Veg_EVT_Type" transparency="40" />
              <Node label="Veg Type - EVT Name" xpath="Realizations/RSContext/Raster[@id='EXVEG']" type="raster" symbology="Existing_Veg_EVT_Name" transparency="40" />
              <Node label="Veg Type - EVT Class" xpath="Realizations/RSContext/Raster[@id='EXVEG']" type="raster" symbology="Existing_Veg_EVT_Class" transparency="40" />
            </Children>
          </Node>
          <Node label="Historic Vegetation">
            <Children collapsed="true">
              <Node label="Veg Type - BPS Type" xpath="Realizations/RSContext/Raster[@id='HISTVEG']" type="raster" symbology="Historic_Veg_BPS_Type" transparency="40" />
              <Node label="Veg Type - BPS Name" xpath="Realizations/RSContext/Raster[@id='HISTVEG']" type="raster" symbology="Historic_Veg_BPS_Name" transparency="40" />
            </Children>
          </Node>
        </Children>
      </Node>
      <Node label="Topography and Derivatives">
        <Children collapsed="true">
          <Node label="DEM (10 m NED)" xpath="Realizations/RSContext/DEM" type="raster" symbology="dem" transparency="40" />
          <Node label="Slope Analysis" xpath="Realizations/RSContext/Raster[@id='SLOPE']" type="raster" symbology="slope" transparency="40" />
          <Node xpathlabel="Name" xpath="Realizations/RSContext/Raster[@id='FA']" type="raster" symbology="flow_accumulation" transparency="40" />
          <Node label="Drainage Area" xpath="Realizations/RSContext/Raster[@id='DA']" type="raster" symbology="drainage_area" transparency="40" />
          <Node label="Detrended DEM (HAND - Height Above Nearest Drainge)" xpath="Realizations/RSContext/Raster[@id='HAND']" type="raster" symbology="hand" transparency="40" />
        </Children>
      </Node>
      <Node label="Hillshade">
        <Children collapsed="false">
          <Node xpathlabel="Name" xpath="Realizations/RSContext/Raster[@id='HILLSHADE']" type="raster" symbology="" />
        </Children>
      </Node>
    </Children>
  </Node>
</Project>