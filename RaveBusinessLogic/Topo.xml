<?xml version="1.0" encoding="utf-8" ?>
<!-- <Project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://xml.riverscapes.xyz/RaveBusinessLogic/XSD/project_explorer.xsd"> -->
<Project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="./XSD/project_explorer.xsd">
  <Name>CHaMP Topo</Name>
  <ProjectType>topo</ProjectType>
  <Node xpathlabel="Name">
    <Children collapsed="false">
      <Node label="Topos">
        <Children>
          <Node label="Auxiliary Survey Data">
            <Children  collapsed="true">
              <Node label="Channel Units">
                <Children>
                  <Node label="Unit ID" xpath="Realizations/Topography/TIN/ChannelUnits" type="polygon" symbology="cu_UnitNum" />
                </Children>
              </Node>
              <Node label="Hawkins Channel Units">
                <Children>
                  <Node label="Tier 1" xpath="Realizations/Topography/TIN/ChannelUnits" type="polygon" symbology="cu_T1" />
                  <Node label="Tier 2" xpath="Realizations/Topography/TIN/ChannelUnits" type="polygon" symbology="cu_T2" />
                </Children>
              </Node>
              <Node label="Grain Size Occular Estimates">
                <Children>
                  <Node label="D16" xpath="Realizations/Topography/TIN/ChannelUnits" type="polygon" symbology="cu_D16" />
                  <Node label="D50" xpath="Realizations/Topography/TIN/ChannelUnits" type="polygon" symbology="cu_D50" />
                  <Node label="D84" xpath="Realizations/Topography/TIN/ChannelUnits" type="polygon" symbology="cu_D84" />
                  <Node label="D90" xpath="Realizations/Topography/TIN/ChannelUnits" type="polygon" symbology="cu_D90" />
                </Children>
              </Node>
            </Children>
          </Node>
    
          <Node label="Topographic Survey Data">
            <Children collapsed="true">
              <Node xpathlabel="Name" xpath="Realizations/SurveyData/SurveyExtents/Vector[@id='survey_extent']" type="polygon" symbology="SurveyExtent" />
              <Node xpathlabel="Name" xpath="Realizations/SurveyData/Vector[@id='breaklines']" type="line" symbology="breaklines" />
              <Node xpathlabel="Name" xpath="Realizations/SurveyData/Vector[@id='topo_points']" type="point" symbology="pts_topo" />
              <Node xpathlabel="Name" xpath="Realizations/SurveyData/Vector[@id='eow_points']" type="point" symbology="pts_eow" />
              <Node xpathlabel="Name" xpath="Realizations/SurveyData/Vector[@id='control_points']" type="point" symbology="pts_control" />
              <Node xpathlabel="Name" xpath="Realizations/SurveyData/Vector[@id='stream_features']" type="point" symbology="pts_streamFeatures" />
              <Node xpathlabel="Name" xpath="Realizations/SurveyData/Vector[@id='error_points']" type="point" symbology="pts_error" />
            </Children>
          </Node>
    
          <Node label="Topographic Derivatives">
            <Children collapsed="true">
              <Node label="DEM Error Model">
                <Children>
                  <Node label="Associated Surfaces">
                    <Children>
                      <Node label="Point Density" xpath="Realizations/Topography/TIN/AssocSurfaces/PointDensity" type="raster" symbology="asPD" transparency="40" />
                      <Node label="Slope Analyis" xpath="Realizations/Topography/TIN/AssocSurfaces/Slope" type="raster" symbology="asSD" transparency="40" />
                      <Node label="Interpolation Error" xpath="Realizations/Topography/TIN/AssocSurfaces/InterpolationError" type="raster" symbology="ErrorDEM" transparency="40" />
                      <Node label="Roughness Height" xpath="Realizations/Topography/TIN/AssocSurfaces/Roughness" type="raster" symbology="" transparency="40" />
                    </Children>
                  </Node>
                  <Node label="Error Model(s)">
                    <Children>
                      <Node label="FIS Error Model" xpath="Realizations/Topography/TIN/AssocSurfaces/ErrSurface" type="raster" symbology="ErrorDEM" transparency="40" />
                    </Children>
                  </Node>
                </Children>
              </Node>
              <Node label="Hydraulics">
                <Children>
                  <Node xpathlabel="Name" xpath="Realizations/Topography/TIN/WaterDepth" type="raster" symbology="wDepth" transparency="40" />
                  <Node xpathlabel="Name" xpath="Realizations/Topography/TIN/WaterSurfaceDEM" type="raster" symbology="wsDEM" transparency="40" />
                  <Node xpathlabel="Name" xpath="Realizations/Topography/TIN/Thalweg" type="line" symbology="thalweg" />
                </Children>
              </Node>
    
              <Node label="Mapping">
                <Children collapsed="true">
                  <Node label="Cross Sections, Profiles and Extents">
                    <Children>
                      <Node label="Surveyed Wetted Extent">
                        <Children>
                          <Node label="Wetted Centerline" xpath="Realizations/Topography/TIN/Stages/Vector[@id='wetted_centerline']" type="line" symbology="wetCL" />
                          <Node label="Wetted Islands" xpath="Realizations/Topography/TIN/Stages/Vector[@id='wetted_islands']" type="polygon" symbology="wetIslands" />
                          <Node label="Wetted Polygon" xpath="Realizations/Topography/TIN/Stages/Vector[@id='wetted_extent']" type="polygon" symbology="wetExtent" />
                          <Node label="X-Sections" xpath="Realizations/Topography/TIN/Stages/Vector[@id='wetted_crosssections']" type="line" symbology="XS_type" />
                          <Node label="XS QA-QC" xpath="Realizations/Topography/TIN/Stages/Vector[@id='wetted_crosssections']" type="line" symbology="XS_qaqc" />
                        </Children>
                      </Node>
                      <Node label="Bankfull Extent">
                        <Children>
                          <Node label="Bankfull Centerline" xpath="Realizations/Topography/TIN/Stages/Vector[@id='bankfull_centerline']" type="line" symbology="bfCL" />
                          <Node label="Bankfull Islands" xpath="Realizations/Topography/TIN/Stages/Vector[@id='bankfull_islands']" type="polygon" symbology="bfIslands" />
                          <Node label="Bankfull Extent Polygon" xpath="Realizations/Topography/TIN/Stages/Vector[@id='bankfull_extent']" type="polygon" symbology="bfExtent" />
                          <Node label="BF X-Sections" xpath="Realizations/Topography/TIN/Stages/Vector[@id='bankfull_crosssections']" type="line" symbology="XS_type" />
                          <Node label="BF XS QA-QC" xpath="Realizations/Topography/TIN/Stages/Vector[@id='bankfull_crosssections']" type="line" symbology="XS_qaqc" />
                        </Children>
                      </Node>
                    </Children>
                  </Node>
    
                </Children>
              </Node>
            </Children>
          </Node>
    
          <Node label="Topography">
            <Children collapsed="false">
              <Node label="TINs">
                <Children>
                  <Node xpathlabel="Name" xpath="Realizations/Topography/TIN" type="tin" symbology="" />
                  <Node xpathlabel="Name" xpath="Realizations/Topography/TIN/WaterSurfaceTIN" type="tin" symbology="" />
                </Children>
              </Node>
              <Node label="Digital Elevation Models">
                <Children>
                  <Node xpathlabel="Name" xpath="Realizations/Topography/TIN/Detrended" type="raster" symbology="detrendDEM" transparency="40" />
                  <Node xpathlabel="Name" xpath="Realizations/Topography/TIN/DEM" type="raster" symbology="DEM" transparency="40"  />
                  <Node xpathlabel="Name" xpath="Realizations/Topography/TIN/DEMHillshade" type="raster" symbology="" />
    
                </Children>
              </Node>
            </Children>
          </Node>
        </Children>
      </Node>
      <Node label="Inputs">
        <Children collapsed="true">
          <Node label="Raw Survey Data">
            <Children>
              <Node label="Breaklines CAD DXF file" xpath="Inputs/Vector[@id='BreaklineDXF']" type="file" />
              <Node label="Survey Instrument File (*.mjf)" xpath="Inputs/InstrumentFile" type="file" />
              <Node label="Auxilary RAW Instrument File" xpath="Inputs/AuxInstrumentFile" type="file" />
            </Children>
          </Node>
          <Node label="Survey Quality QA/QC">
            <Children>
              <Node label="Survey Quality SQLite Database" xpath="Inputs/SurveyQualityDB" type="file" />
    
            </Children>
          </Node>
        </Children>
      </Node>
    </Children>
  </Node>
</Project>