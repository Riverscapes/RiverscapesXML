              <Node label="Integrated VB Width" xpath="Outputs/Geopackage/Layers/Vector[@lyrName='vbet_igos']" type="point" symbology="MetricVBET_Width"  />
              <Node label="Moving Window Analysis Distance" xpath="Outputs/Geopackage/Layers/Vector[@lyrName='vbet_igos']" type="point" symbology="metric_moving_window_distance"  />
