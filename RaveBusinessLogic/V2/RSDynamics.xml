<?xml version="1.0" encoding="utf-8"?>
<Project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://xml.riverscapes.net/RaveBusinessLogic/XSD/project_explorer.xsd">
        <Name>Riverscapes Dynamics</Name>
        <ProjectType>RSDynamics</ProjectType>
        <Node xpathlabel="Name">
                <Children collapsed="false">
                        <Node label="Outputs">
                                <Children collapsed="true">
                                        <Node label="95% Confidence Interval">
                                                <Children>
                                                        <Repeater label="Area"
                                                                xpath="Realizations/Realization/Outputs/Geopackage[@id='RSDYNAMICS_DGOS']/Layers/Vector[starts-with(@lyrName, 'vw_area_95_')]">
                                                                <Node xpathlabel="Name"
                                                                        type="polygon"
                                                                        symbology="epoch_area" />
                                                        </Repeater>
                                                        <Repeater label="Width"
                                                                xpath="Realizations/Realization/Outputs/Geopackage[@id='RSDYNAMICS_DGOS']/Layers/Vector[starts-with(@lyrName, 'vw_width_95_')]">
                                                                <Node xpathlabel="Name"
                                                                        type="polygon"
                                                                        symbology="epoch_width" />
                                                        </Repeater>
                                                        <Repeater label="Percent"
                                                                xpath="Realizations/Realization/Outputs/Geopackage[@id='RSDYNAMICS_DGOS']/Layers/Vector[starts-with(@lyrName, 'vw_pc_95_')]">
                                                                <Node xpathlabel="Name"
                                                                        type="polygon"
                                                                        symbology="epoch_pc" />
                                                        </Repeater>
                                                </Children>
                                        </Node>
                                        <Node label="68% Confidence Interval">
                                                <Children>
                                                        <Repeater label="Area"
                                                                xpath="Realizations/Realization/Outputs/Geopackage[@id='RSDYNAMICS_DGOS']/Layers/Vector[starts-with(@lyrName, 'vw_area_68_')]">
                                                                <Node xpathlabel="Name"
                                                                        type="polygon"
                                                                        symbology="epoch_area" />
                                                        </Repeater>
                                                        <Repeater label="Width"
                                                                xpath="Realizations/Realization/Outputs/Geopackage[@id='RSDYNAMICS_DGOS']/Layers/Vector[starts-with(@lyrName, 'vw_width_68_')]">
                                                                <Node xpathlabel="Name"
                                                                        type="polygon"
                                                                        symbology="epoch_width" />
                                                        </Repeater>
                                                        <Repeater label="Percent"
                                                                xpath="Realizations/Realization/Outputs/Geopackage[@id='RSDYNAMICS_DGOS']/Layers/Vector[starts-with(@lyrName, 'vw_pc_68_')]">
                                                                <Node xpathlabel="Name"
                                                                        type="polygon"
                                                                        symbology="epoch_pc" />
                                                        </Repeater>
                                                </Children>
                                        </Node>
                                </Children>
                        </Node>
                        <Node label="Epoch Rasters">
                                <Children>
                                        <Repeater label="Vegetation"
                                                xpath="Realizations/Realization/Outputs/Raster[contains(@id, 'VEGETATION')]">
                                                <Node xpathlabel="Name"
                                                        type="raster"
                                                />

                                        </Repeater>
                                        <Repeater label="Water"
                                                xpath="Realizations/Realization/Outputs/Raster[contains(@id, 'WATER')]">
                                                <Node xpathlabel="Name"
                                                        type="raster"
                                                />

                                        </Repeater>
                                        <Repeater label="Alluvial"
                                                xpath="Realizations/Realization/Outputs/Raster[contains(@id, 'ALLUVIAL')]">
                                                <Node xpathlabel="Name"
                                                        type="raster"
                                                />

                                        </Repeater>
                                </Children>
                        </Node>
                        <Node label="Classified Rasters">
                                <Children collapsed="true">
                                        <Repeater label="Vegetation"
                                                xpath="Realizations/Realization/Inputs/Raster[starts-with(@id, 'CLASS_VEG')]">
                                                <Node xpathlabel="Name" type="raster"
                                                        symbology="class_bin_veg" />
                                        </Repeater>
                                        <Repeater label="Water"
                                                xpath="Realizations/Realization/Inputs/Raster[starts-with(@id, 'CLASS_WATER')]">
                                                <Node xpathlabel="Name" type="raster"
                                                        symbology="class_bin_water" />
                                        </Repeater>
                                        <Repeater label="Active"
                                                xpath="Realizations/Realization/Inputs/Raster[starts-with(@id, 'CLASS_ALLUVIAL')]">
                                                <Node xpathlabel="Name" type="raster"
                                                        symbology="class_bin_active" />
                                        </Repeater>
                                </Children>
                        </Node>
                        <Node xpathlabel="Name"
                                xpath="Realizations/Realization/Inputs/Raster[@id='HILLSHADE']"
                                type="raster" transparency="40" symbology="hillshade" id="hillshade" />
                </Children>
        </Node>
        <Views default="DEFAULT">
                <View name="Primary Outputs" id="DEFAULT">
                        <Layers>
                                <Layer id="vbet_igos_stdev" />
                                <Layer id="DGO_white" />
                                <Layer id="alluvial_30yr" />
                                <Layer id="hillshade" />
                        </Layers>
                </View>
        </Views>
</Project>